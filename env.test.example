# Test Environment Configuration
# Copy this file to .env.test and update with your test database credentials

# Node Environment
NODE_ENV=test

# Supabase Test Configuration
SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_SERVICE_KEY=your-test-service-key-here

# Test Database (if different from main Supabase project)
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/sojo_test

# Test Configuration
TEST_TIMEOUT=10000
TEST_PARALLEL=false

# Mock External Services (for testing)
MOCK_EMAIL_SERVICE=true
MOCK_SMS_SERVICE=true
MOCK_PAYMENT_SERVICE=true

# Test Data Cleanup
CLEANUP_AFTER_TESTS=true
CLEANUP_INTERVAL=300000

# Logging
LOG_LEVEL=error
LOG_TEST_OUTPUT=false
